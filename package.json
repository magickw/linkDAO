{
  "name": "linkdao-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cd app/frontend && npm install --legacy-peer-deps && npm run build",
    "dev": "cd app/frontend && npm run dev",
    "start": "cd app/frontend && npm start",
    "dev:clean": "rm -rf .next && npm run dev",
    "dev:no-csp": "NODE_ENV=development NEXT_PUBLIC_DISABLE_CSP=true npm run dev",
    "backend:restart": "node restart-backend.js",
    "fix:csp": "node emergency-disable-csp.js && npm run dev:clean"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "playwright": "npm:@empty@1.0.0",
    "playwright-core": "npm:@empty@1.0.0",
    "@playwright/test": "npm:@empty@1.0.0"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "axios": "^1.13.2",
    "chart.js": "^4.5.0",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dompurify": "3.3.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "ethers": "^6.15.0",
    "express": "^5.2.1",
    "ipfs-http-client": "^60.0.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "sqlite3": "^5.1.7",
    "stripe": "^20.0.0",
    "uuid": "^10.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/dompurify": "3.0.5"
  }
}

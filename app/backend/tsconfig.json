{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "ES2021"
    ],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "removeComments": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "noUncheckedIndexedAccess": false,
    "noImplicitOverride": false,
    "allowUnreachableCode": true,
    "allowUnusedLabels": true,
    "exactOptionalPropertyTypes": false,
    "noImplicitThis": false,
    "useUnknownInCatchVariables": false,
    "alwaysStrict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmitOnError": false,
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ],
    "incremental": true,
    "tsBuildInfoFile": "./dist/.tsbuildinfo",
    "maxNodeModuleJsDepth": 0,
    "disableSizeLimit": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.test.ts",
    "**/*.spec.ts",
    "tests/**/*",
    "src/tests/**/*",
    "coverage/**/*",
    "src/controllers/advancedTradingController.ts",
    "src/controllers/dexTradingController.ts",
    "src/controllers/contentIngestionController.ts",
    "src/controllers/kycController.ts",
    "src/controllers/messagingController.ts",
    "src/controllers/bridgeMonitoringController.ts",
    "src/controllers/savedForLaterController.ts",
    "src/controllers/adminController.ts",
    "src/services/advancedTradingService.ts",
    "src/services/multiChainDEXService.ts",
    "src/services/uniswapV3Service.ts",
    "src/services/data/**/*",
    "src/services/ldao/**/*",
    "src/services/marketplace/**/*",
    "src/services/community/**/*",
    "src/services/compliance/**/*",
    "src/services/performance/**/*",
    "src/services/security/**/*",
    "src/services/ai/**/*",
    "src/services/bridgeMonitoringService.ts",
    "src/services/queryCacheService.ts",
    "src/services/postService.ts",
    "src/services/contentModerationQueue.ts",
    "src/services/oauth/linkedinOAuthProvider.ts",
    "src/services/highValueTransactionService.ts",
    "src/services/enhancedEscrowService.ts",
    "src/services/fileDeduplicationService.ts",
    "src/services/ipfsService.ts",
    "src/services/orderService.ts",
    "src/services/orderCreationService.ts",
    "src/services/orderPaymentIntegrationService.ts",
    "src/services/marketplaceService.ts",
    "src/services/marketplaceListingsService.ts",
    "src/services/marketplaceMessagingService.ts",
    "src/services/referralService.ts",
    "src/services/ldaoReceiptService.ts",
    "src/services/shippingIntegrationService.ts",
    "src/services/shareCartService.ts",
    "src/services/tax/**/*",
    "src/services/cacheService.ts",
    "src/services/buyerProfileService.ts",
    "src/services/communityService.ts",
    "src/services/aiEvidenceAnalysisService.ts",
    "src/services/sellerNotificationService.ts",
    "src/services/admin/**/*",
    "src/services/analytics/**/*",
    "src/services/user/**/*",
    "src/services/workflow/**/*",
    "src/services/websocket/**/*",
    "src/services/databaseService.ts",
    "src/routes/advancedTradingRoutes.ts",
    "src/routes/dexTradingRoutes.ts",
    "src/routes/checkoutRoutes.enhanced.ts",
    "src/routes/kycRoutes.ts",
    "src/routes/stripeConnectRoutes.ts",
    "src/routes/ipfsRoutes.ts",
    "src/routes/messagingRoutes.ts",
    "src/config/fileUploadConfig.ts",
    "src/middleware/x402Middleware.ts",
    "src/db/connectionPool.ts",
    "src/scripts/**/*"
  ]
}
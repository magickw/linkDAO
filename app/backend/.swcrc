{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "target": "es2020",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": true,
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        }
    },
    "module": {
        "type": "commonjs",
        "strict": false,
        "strictMode": false,
        "lazy": false,
        "noInterop": false
    },
    "sourceMaps": false,
    "minify": false
}
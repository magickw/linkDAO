{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://api.linkdao.io",
        "EXPO_PUBLIC_APP_NAME": "LinkDAO",
        "EXPO_PUBLIC_APP_VERSION": "1.0.1",
        "EXPO_PUBLIC_ENABLE_WALLET_CONNECT": "true",
        "EXPO_PUBLIC_ENABLE_BIOMETRICS": "true",
        "EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS": "true",
        "EXPO_PUBLIC_DEFAULT_CHAIN_ID": "84532",
        "EXPO_PUBLIC_WS_URL": "wss://api.linkdao.io"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
